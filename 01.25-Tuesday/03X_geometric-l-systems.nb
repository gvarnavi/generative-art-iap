    RowBox[{
     RowBox[{"drawingLetters", "=", 
      RowBox[{"RandomSample", "[", 
       RowBox[{"letters", ",", 
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"Length", "[", "letters", "]"}], "-", "1"}], ",", 
           RowBox[{"Length", "[", "letters", "]"}]}], "}"}], "]"}]}], "]"}]}],
      ";", "\[IndentingNewLine]", 
     RowBox[{"rs", ":=", 
      RowBox[{"RandomSample", "[", 
       RowBox[{
        RowBox[{"Join", "[", 
         RowBox[{"letters", ",", 
          RowBox[{"{", 
           RowBox[{"\"\<+\>\"", ",", "\"\<-\>\""}], "}"}]}], "]"}], ",", 
        RowBox[{"l", "=", 
         RowBox[{"RandomInteger", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Ceiling", "[", 
             RowBox[{
              RowBox[{"Length", "[", "letters", "]"}], "/", "2"}], "]"}], ",", 
            RowBox[{
             RowBox[{"Length", "[", "letters", "]"}], "+", "2"}]}], "}"}], 
          "]"}]}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"SubstitutionSystem", "[", 
        RowBox[{"MapThread", "[", 
         RowBox[{"Rule", ",", 
          RowBox[{"{", 
