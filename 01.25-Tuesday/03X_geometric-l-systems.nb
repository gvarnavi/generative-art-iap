    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"segmentPos", "=", 
      RowBox[{"Sort", "[", 
       RowBox[{"RandomInteger", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"1", ",", 
           RowBox[{"Length", "[", "parentOne", "]"}]}], "}"}], ",", "2"}], 
        "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"chance", "<", "rate"}], " ", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{
         "Only", " ", "crossover", " ", "according", " ", "to", " ", "a", " ",
           "probability", " ", "rate", " ", "r"}], "\[Element]", 
         RowBox[{"[", 
          RowBox[{"0", ",", "1"}], "]"}]}], "*)"}], ",", 
