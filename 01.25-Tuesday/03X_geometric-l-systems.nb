     RowBox[{"{", 
      RowBox[{"p1x_", ",", "p1y_"}], "}"}]}], "]"}], ":=", 
   RowBox[{
    RowBox[{"ii", "[", 
     RowBox[{"[", 
      RowBox[{"p1x", ",", "p1y"}], "]"}], "]"}], "+", 
    RowBox[{"ii", "[", 
     RowBox[{"[", 
      RowBox[{"p0x", ",", "p0y"}], "]"}], "]"}], "-", 
    RowBox[{"ii", "[", 
     RowBox[{"[", 
      RowBox[{"p1x", ",", "p0y"}], "]"}], "]"}], "-", 
    RowBox[{"ii", "[", 
     RowBox[{"[", 
      RowBox[{"p0x", ",", "p1y"}], "]"}], "]"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"PartitionBoxes", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"rows_", ",", "cols_"}], "}"}], ",", "size_"}], "]"}], ":=", 
   RowBox[{"Transpose", "/@", 
    RowBox[{"Tuples", "[", 
     RowBox[{"{", 
      RowBox[{
