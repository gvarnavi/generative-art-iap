        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{"1", ",", "rows", ",", "size"}], "]"}], ",", "2", ",", 
         "1"}], "]"}], ",", 
       RowBox[{"Partition", "[", 
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{"1", ",", "cols", ",", "size"}], "]"}], ",", "2", ",", 
         "1"}], "]"}]}], "}"}], "]"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"PixelCountsAtSize", "[", 
    RowBox[{
