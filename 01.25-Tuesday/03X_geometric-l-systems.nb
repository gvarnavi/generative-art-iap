            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"StringJoin", "@", 
               RowBox[{"If", "[", 
                RowBox[{"pushPop", ",", 
                 RowBox[{"Fold", "[", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"Insert", "[", 
                    RowBox[{"#1", ",", 
                    RowBox[{"#2", "[", 
                    RowBox[{"[", "1", "]"}], "]"}], ",", 
                    RowBox[{"#2", "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}], "]"}], "&"}], ",", "rs",
                    ",", 
                   RowBox[{"Thread", "[", 
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{"{", 
                    RowBox[{"\"\<[\>\"", ",", "\"\<]\>\""}], "}"}], ",", 
                    RowBox[{"Sort", "[", 
                    RowBox[{"Take", "[", 
                    RowBox[{
                    RowBox[{"RandomSample", "[", 
                    RowBox[{"Range", "[", "l", "]"}], "]"}], ",", "2"}], 
                    "]"}], "]"}]}], "}"}], "]"}]}], "]"}], ",", "rs"}], 
                "]"}]}], ",", 
              RowBox[{"{", 
               RowBox[{"Length", "[", "drawingLetters", "]"}], "}"}]}], 
             "]"}]}], "}"}]}], "]"}], "]"}], ",", "drawingLetters", ",", 
