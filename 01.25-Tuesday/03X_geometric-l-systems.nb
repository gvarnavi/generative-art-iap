Cell[BoxData[
 RowBox[{
  RowBox[{"generateReplacementRules", "[", 
   RowBox[{"letters_", ",", "n_", ",", 
    RowBox[{"pushPop_", ":", "False"}]}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"l", ",", "rs", ",", "drawingLetters"}], "}"}], ",", 
