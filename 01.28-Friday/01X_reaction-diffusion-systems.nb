Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"spectralCahnHilliard", "[", 
    RowBox[{"u_", ",", " ", "kappa_", ",", "  ", "dt_"}], "]"}], "[", 
   RowBox[{"kMagnitudeSquared_", ",", "kMagnitudeFourth_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"fourierDf", "=", 
       RowBox[{"Fourier", "[", 
        RowBox[{"freeEnergyDerivative", "[", "u", "]"}], "]"}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"fourierU", "=", " ", 
       RowBox[{"Fourier", "[", "u", "]"}]}], " ", ",", "\[IndentingNewLine]", 
      "part1", ",", "\[IndentingNewLine]", "part2"}], "\[IndentingNewLine]", 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"part1", "=", 
      RowBox[{"dt", " ", "kMagnitudeSquared", " ", "fourierDf"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"part2", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"fourierU", "-", " ", "part1"}], ")"}], "/", 
       RowBox[{"(", 
        RowBox[{"1", " ", "+", " ", 
         RowBox[{"dt", " ", "kappa", " ", "kMagnitudeFourth"}]}], ")"}]}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Re", "[", 
      RowBox[{"InverseFourier", "[", "part2", "]"}], "]"}]}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.820410525393387*^9, 3.820410530129777*^9}, {
