Cell[CellGroupData[{

Cell["\<\
First, we setup a finite grid of wave-vectors (dual-lattice vectors for the \
the domain we wish to solve)\
\>", "Item",
 CellChangeTimes->{{3.820403884241529*^9, 
  3.820403919142229*^9}},ExpressionUUID->"19bc1915-3145-4a19-b72c-\
62e98e83967c"],

Cell[BoxData[
 RowBox[{
  RowBox[{"spectralWavevector", "[", "nSize_", "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "baseVec", "}"}], ",", "\n", 
    RowBox[{
     RowBox[{"baseVec", " ", "=", " ", 
      RowBox[{"Range", "[", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"(", 
          RowBox[{"1", "+", 
           RowBox[{"1", "/", "nSize"}]}], ")"}]}], ",", "1", ",", 
        RowBox[{"2", "/", "nSize"}]}], "]"}]}], ";", "  ", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"EvenQ", "[", "nSize", "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"baseVec", " ", "=", " ", 
         RowBox[{"Range", "[", 
          RowBox[{
           RowBox[{"-", "1"}], ",", 
           RowBox[{"1", "/", "1"}], ",", 
           RowBox[{"2", "/", "nSize"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"baseVec", " ", "=", " ", 
         RowBox[{"Rest", "[", "baseVec", "]"}]}], ";", "\[IndentingNewLine]", 
        
        RowBox[{"baseVec", " ", "=", " ", 
         RowBox[{"RotateLeft", "[", 
          RowBox[{"baseVec", ",", 
           RowBox[{
            RowBox[{"nSize", "/", "2"}], " ", "-", "1"}]}], "]"}]}]}], ",", 
       " ", "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"baseVec", " ", "=", " ", 
         RowBox[{"Range", "[", 
          RowBox[{
           RowBox[{"-", 
            RowBox[{"(", 
             RowBox[{"1", "+", " ", 
              RowBox[{"1", "/", "nSize"}]}], ")"}]}], ",", 
           RowBox[{"1", "+", " ", 
            RowBox[{"1", "/", "nSize"}]}], ",", 
           RowBox[{"2", "/", "nSize"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"baseVec", " ", "=", " ", 
         RowBox[{"Most", "[", 
          RowBox[{"Rest", "[", "baseVec", "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"baseVec", " ", "=", " ", 
         RowBox[{"RotateLeft", "[", 
          RowBox[{"baseVec", ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"nSize", "-", "1"}], ")"}], "/", "2"}]}], "]"}]}]}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"N", "[", "Pi", "]"}], " ", "baseVec"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.820403218192875*^9, 3.82040324456791*^9}, {
  3.8204032779529552`*^9, 3.820403298144588*^9}},
 CellLabel->"In[9]:=",ExpressionUUID->"c452d1ed-7ff8-41af-a5b6-dc6ff9bc6dcb"],

Cell[BoxData[
 RowBox[{
  RowBox[{"spectralWavevectorSquared", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"nx_Integer", ",", " ", "ny_Integer"}], "}"}], ",", 
    RowBox[{"preMultiply_", ":", "True"}]}], "]"}], ":=", " ", "\n", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"kx", ",", "ky", ",", "kMagnitudeSquared"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"kx", " ", "=", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{"preMultiply", ",", " ", "nx", ",", "1"}], "]"}], " ", 
       RowBox[{"Transpose", "[", 
        RowBox[{"ConstantArray", "[", 
         RowBox[{
          RowBox[{"spectralWavevector", "[", "nx", "]"}], ",", "ny"}], "]"}], 
        "]"}]}]}], ";", 
     RowBox[{"ky", " ", "=", " ", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{"preMultiply", ",", " ", "ny", ",", "1"}], "]"}], " ", 
       RowBox[{"ConstantArray", "[", 
        RowBox[{
         RowBox[{"spectralWavevector", "[", "ny", "]"}], ",", "nx"}], 
        "]"}]}]}], ";", 
     RowBox[{"kMagnitudeSquared", "  ", "=", " ", 
      RowBox[{
       RowBox[{"kx", "^", "2"}], " ", "+", " ", 
       RowBox[{"ky", "^", "2"}]}]}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.820403477692196*^9, 3.8204035085545464`*^9}, {
   3.8204035585104713`*^9, 3.8204035708732367`*^9}, 3.8204036723967257`*^9, {
