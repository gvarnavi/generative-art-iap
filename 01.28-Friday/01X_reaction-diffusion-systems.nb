Cell["Next, we formulate our pde in Fourier space", "Item",
 CellChangeTimes->{{3.8204039645671864`*^9, 
  3.820403975934651*^9}},ExpressionUUID->"6e7f3bf4-8b89-4994-978c-\
c0bd99e048b0"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"spectralGrayScott", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"u_", ",", " ", "v_"}], "}"}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"diffU_", ",", " ", "diffV_", ",", "f_", ",", " ", "k_"}], 
      "}"}], ",", "  ", "dt_"}], "]"}], "[", "kMagnitudeSquared_", "]"}], ":=",
   "\[IndentingNewLine]", 
  RowBox[{"Block", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"fourierU", " ", "=", " ", 
       RowBox[{"Fourier", "[", "u", "]"}]}], ",", " ", "\[IndentingNewLine]", 
      
      RowBox[{"fourierV", " ", "=", "  ", 
       RowBox[{"Fourier", "[", "v", "]"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"uvSquared", "  ", "=", " ", 
       RowBox[{"u", " ", 
        RowBox[{"v", "^", "2"}]}]}], ",", " ", "\[IndentingNewLine]", 
      "fourieruvSquared"}], " ", "\[IndentingNewLine]", "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"fourieruvSquared", "=", 
      RowBox[{"Fourier", "[", " ", "uvSquared", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Re", "@", 
        RowBox[{"InverseFourier", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"fourierU", " ", "+", "  ", 
            RowBox[{"dt", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"Fourier", "[", 
                RowBox[{"f", 
                 RowBox[{"(", 
                  RowBox[{"1", "-", "u"}], ")"}]}], "]"}], " ", "-", " ", 
               "fourieruvSquared"}], ")"}]}]}], ")"}], "/", 
          RowBox[{"(", 
           RowBox[{"1", " ", "+", " ", 
            RowBox[{"diffU", "*", "dt", "*", "kMagnitudeSquared"}]}], ")"}]}],
          "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"Re", "@", 
        RowBox[{"InverseFourier", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"fourierV", " ", "+", " ", 
            RowBox[{"dt", 
             RowBox[{"(", 
              RowBox[{"fourieruvSquared", "-", 
               RowBox[{"Fourier", "[", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"f", "+", " ", "k"}], ")"}], "v"}], "]"}]}], 
              ")"}]}]}], ")"}], "/", 
          RowBox[{"(", 
           RowBox[{"1", " ", "+", " ", 
            RowBox[{"diffV", "*", "dt", "*", "kMagnitudeSquared"}]}], ")"}]}],
          "]"}]}]}], "\[IndentingNewLine]", "}"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.820403322594974*^9, 3.82040336981628*^9}, {
  3.820405187682513*^9, 3.8204052034992723`*^9}},
 CellLabel->"In[12]:=",ExpressionUUID->"209a442e-b9e6-4dcc-aeb3-b7b0af0ef6ed"]
