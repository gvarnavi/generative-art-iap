Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"frames", "[", 
    RowBox[{"\"\<gs\>\"", ",", "\"\<single-seed\>\"", ",", "\"\<disk\>\""}], 
    "]"}], "=", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"vRange", "=", 
        RowBox[{"MinMax", "[", 
         RowBox[{
          RowBox[{"vfun", "[", 
           RowBox[{
           "\"\<gs\>\"", ",", "\"\<single-seed\>\"", ",", "\"\<disk\>\""}], 
           "]"}], "[", "\"\<ValuesOnGrid\>\"", "]"}], "]"}]}], ",", 
       RowBox[{"sol", "=", 
        RowBox[{"vfun", "[", 
         RowBox[{
         "\"\<gs\>\"", ",", "\"\<single-seed\>\"", ",", "\"\<disk\>\""}], 
         "]"}]}], ",", 
       RowBox[{"reg", "=", 
        RowBox[{"region", "[", "\"\<disk\>\"", "]"}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"ContourPlot", "[", 
        RowBox[{
         RowBox[{"sol", "[", 
          RowBox[{"t", ",", "x", ",", "y"}], "]"}], ",", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"x", ",", "y"}], "}"}], "\[Element]", "reg"}], ",", 
         RowBox[{"PlotRange", "\[Rule]", "All"}], ",", 
         RowBox[{"Frame", "\[Rule]", "None"}], ",", 
         RowBox[{"Axes", "\[Rule]", "None"}], ",", 
         RowBox[{"ColorFunction", "\[Rule]", " ", "Hue"}], ",", 
         RowBox[{"Contours", "\[Rule]", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"Subdivide", "[", 
              RowBox[{"#1", ",", "#2", ",", "4"}], "]"}], "&"}], "@@", 
            "vRange"}], ")"}]}], ",", 
         RowBox[{"PlotPoints", "\[Rule]", "50"}], ",", 
         RowBox[{"MaxRecursion", "\[Rule]", "3"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"t", ",", 
         RowBox[{"Subdivide", "[", 
          RowBox[{"90", ",", "1930", ",", "23"}], "]"}]}], "}"}]}], "]"}]}], 
