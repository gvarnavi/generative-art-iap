c8e07073b241"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"eqn", "[", "\"\<gray-scott\>\"", "]"}], "[", 
    RowBox[{"Du_", ",", "Dv_", ",", "f_", ",", "k_"}], "]"}], "=", " ", 
   RowBox[{"{", "\n", "    ", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        SuperscriptBox["u", 
         TagBox[
          RowBox[{"(", 
           RowBox[{"1", ",", "0", ",", "0"}], ")"}],
          Derivative],
         MultilineFunction->None], "[", 
        RowBox[{"t", ",", " ", "x", ",", " ", "y"}], "]"}], " ", "+", " ", 
       "\n", "      ", 
       RowBox[{
        RowBox[{"Inactive", "[", "Div", "]"}], "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", "Du"}], " ", 
           RowBox[{
            RowBox[{"Inactive", "[", "Grad", "]"}], "[", 
            RowBox[{
             RowBox[{"u", "[", 
              RowBox[{"t", ",", " ", "x", ",", " ", "y"}], "]"}], ",", " ", 
             RowBox[{"{", 
              RowBox[{"x", ",", " ", "y"}], "}"}]}], "]"}]}], ")"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", " ", "y"}], "}"}]}], "]"}], " ", "+", "\n", 
       "      ", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           RowBox[{"v", "[", 
            RowBox[{"t", ",", " ", "x", ",", " ", "y"}], "]"}], "^", "2"}], 
          " ", "+", " ", "f"}], ")"}], " ", 
        RowBox[{"u", "[", 
         RowBox[{"t", ",", " ", "x", ",", " ", "y"}], "]"}]}]}], " ", "==", 
      " ", "f"}], ",", " ", "\n", "    ", 
     RowBox[{
      RowBox[{
       RowBox[{
        SuperscriptBox["v", 
         TagBox[
          RowBox[{"(", 
           RowBox[{"1", ",", "0", ",", "0"}], ")"}],
          Derivative],
         MultilineFunction->None], "[", 
        RowBox[{"t", ",", " ", "x", ",", " ", "y"}], "]"}], " ", "+", " ", 
       "\n", "      ", 
       RowBox[{
        RowBox[{"Inactive", "[", "Div", "]"}], "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", "Dv"}], " ", 
           RowBox[{
            RowBox[{"Inactive", "[", "Grad", "]"}], "[", 
            RowBox[{
             RowBox[{"v", "[", 
              RowBox[{"t", ",", " ", "x", ",", " ", "y"}], "]"}], ",", " ", 
             RowBox[{"{", 
              RowBox[{"x", ",", " ", "y"}], "}"}]}], "]"}]}], ")"}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"x", ",", " ", "y"}], "}"}]}], "]"}], " ", "+", "\n", 
       "      ", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           RowBox[{"-", 
            RowBox[{"u", "[", 
             RowBox[{"t", ",", " ", "x", ",", " ", "y"}], "]"}]}], " ", 
           RowBox[{"v", "[", 
            RowBox[{"t", ",", " ", "x", ",", " ", "y"}], "]"}]}], " ", "+", 
          " ", "f", " ", "+", " ", "k"}], ")"}], " ", 
        RowBox[{"v", "[", 
         RowBox[{"t", ",", " ", "x", ",", " ", "y"}], "]"}]}]}], " ", "==", 
      " ", "0"}]}], "\n", "    ", "}"}]}], " ", ";"}]], "Input",
 CellChangeTimes->{{3.788125356993507*^9, 3.788125363675095*^9}, 
   3.788171327981889*^9, 3.788171368238678*^9, {3.820382273441039*^9, 
   3.820382370090754*^9}, 3.851869700376512*^9},
 CellLabel->"In[2]:=",ExpressionUUID->"f2ccf004-919e-40e7-bafe-62436ae70bdb"]
}, Open  ]]
}, Open  ]],

