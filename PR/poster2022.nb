                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"lambda", " ", "+", " ", 
                   RowBox[{"a", " ", "z", " ", 
                    RowBox[{"Conjugate", "[", "z", "]"}]}], "+", 
                   RowBox[{"b", " ", 
                    RowBox[{"Re", "[", 
