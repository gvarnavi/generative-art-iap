              RowBox[{"ty", "\[LessEqual]", 
               RowBox[{"dim", "[", 
                RowBox[{"[", "2", "]"}], "]"}]}]}], ",", 
             RowBox[{
              RowBox[{"bins", "[", 
               RowBox[{"[", 
                RowBox[{"tx", ",", "ty"}], "]"}], "]"}], "+=", "1"}]}], "]"}],
