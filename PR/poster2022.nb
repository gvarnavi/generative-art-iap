        RowBox[{"d", ",", "_Real"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"p", ",", "_Integer"}], "}"}]}], "}"}], ",", 
     RowBox[{"Block", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "bins", ",", "dim", ",", "x", ",", "y", ",", "tx", ",", "ty", ",", 
