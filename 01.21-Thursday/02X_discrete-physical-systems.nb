Cell[BoxData[
 RowBox[{
  RowBox[{"phaseOrderingNonConserved", "[", 
   RowBox[{"d_", ",", "t_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "separate", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"separate", "[", 
       RowBox[{
       "x_", ",", "n_", ",", "e_", ",", "s_", ",", "w_", ",", "ne_", ",", 
