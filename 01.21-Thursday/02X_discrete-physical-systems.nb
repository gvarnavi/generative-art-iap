          RowBox[{
           RowBox[{"RotateRight", "[", 
            RowBox[{"lat", ",", " ", "#"}], "]"}], "&"}], ",", " ", 
          "upToNNNN"}], "]"}], ",", " ", "2"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"rule", "[", "13", "]"}], "=", "1"}], ";", 
     RowBox[{
      RowBox[{"rule", "[", "14", "]"}], "=", "0"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"rule", "[", "x_", "]"}], ":=", 
      RowBox[{"Floor", "[", 
       RowBox[{"x", "/", "14"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Attributes", "[", "rule", "]"}], "=", "Listable"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"testThreeD", "=", 
      RowBox[{"rule", "[", 
       RowBox[{"Moore", "[", 
        RowBox[{"Plus", ",", " ", "testThreeD"}], "]"}], "]"}]}]}]}], 
   "]"}]}]], "Input",
